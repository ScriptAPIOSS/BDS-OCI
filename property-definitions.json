{
  "server-name": {
    "env": "SERVER_NAME",
    "type": "string"
  },
  "gamemode": {
    "env": "GAMEMODE",
    "type": "string",
    "allowed": [
      "survival",
      "creative",
      "adventure"
    ],
    "mappings": {
      "0": "survival",
      "1": "creative",
      "2": "adventure"
    }
  },
  "force-gamemode": {
    "env": "FORCE_GAMEMODE",
    "type": "bool",
    "allowed": [
      "true",
      "false"
    ]
  },
  "difficulty": {
    "env": "DIFFICULTY",
    "type": "string",
    "allowed": [
      "easy",
      "peaceful",
      "normal",
      "hard"
    ],
    "mappings": {
      "0": "peaceful",
      "1": "easy",
      "2": "normal",
      "3": "hard"
    }
  },
  "allow-cheats": {
    "env": "ALLOW_CHEATS",
    "type": "bool",
    "allowed": [
      "true",
      "false"
    ]
  },
  "max-players": {
    "env": "MAX_PLAYERS",
    "type": "int32"
  },
  "online-mode": {
    "env": "ONLINE_MODE",
    "type": "bool",
    "allowed": [
      "true",
      "false"
    ]
  },
  "allow-list": {
    "env": "ALLOW_LIST",
    "type": "bool",
    "allowed": [
      "true",
      "false"
    ]
  },
  "server-port": {
    "env": "SERVER_PORT",
    "type": "int32"
  },
  "server-portv6": {
    "env": "SERVER_PORT_V6",
    "type": "int32"
  },
  "enable-lan-visibility": {
    "env": "ENABLE_LAN_VISIBILITY",
    "type": "bool",
    "allowed": [
      "true",
      "false"
    ]
  },
  "view-distance": {
    "env": "VIEW_DISTANCE",
    "type": "int32",
    "note": "must be larger than 5"
  },
  "tick-distance": {
    "env": "TICK_DISTANCE",
    "type": "int32",
    "note": "must be between 4 and 12 inclusive"
  },
  "player-idle-timeout": {
    "env": "PLAYER_IDLE_TIMEOUT",
    "type": "int32"
  },
  "max-threads": {
    "env": "MAX_THREADS",
    "type": "int32"
  },
  "level-name": {
    "env": "LEVEL_NAME",
    "type": "string"
  },
  "level-seed": {
    "env": "LEVEL_SEED",
    "type": "string"
  },
  "default-player-permission-level": {
    "env": "DEFAULT_PLAYER_PERMISSION_LEVEL",
    "type": "string",
    "allowed": [
      "visitor",
      "member",
      "operator"
    ]
  },
  "texturepack-required": {
    "env": "TEXTUREPACK_REQUIRED",
    "type": "bool",
    "allowed": [
      "true",
      "false"
    ]
  },
  "content-log-file-enabled": {
    "env": "CONTENT_LOG_FILE_ENABLED",
    "type": "bool",
    "allowed": [
      "true",
      "false"
    ]
  },
  "compression-threshold": {
    "env": "COMPRESSION_THRESHOLD",
    "type": "int32"
  },
  "compression-algorithm": {
    "env": "COMPRESSION_ALGORITHM",
    "type": "string",
    "allowed": [
      "zlib",
      "snappy"
    ]
  },
  "server-authoritative-movement": {
    "env": "SERVER_AUTHORITATIVE_MOVEMENT",
    "type": "string",
    "allowed": [
      "server-auth",
      "client-auth",
      "server-auth-with-rewind"
    ],
    "mappings": {
      "true": "server-auth",
      "false": "client-auth"
    }
  },
  "player-movement-score-threshold": {
    "env": "PLAYER_MOVEMENT_SCORE_THRESHOLD",
    "type": "float"
  },
  "player-movement-action-direction-threshold": {
    "env": "PLAYER_MOVEMENT_ACTION_DIRECTION_THRESHOLD",
    "type": "float"
  },
  "player-movement-distance-threshold": {
    "env": "PLAYER_MOVEMENT_DISTANCE_THRESHOLD",
    "type": "float"
  },
  "player-movement-duration-threshold-in-ms": {
    "env": "PLAYER_MOVEMENT_DURATION_THRESHOLD_IN_MS",
    "type": "int32"
  },
  "correct-player-movement": {
    "env": "CORRECT_PLAYER_MOVEMENT",
    "type": "bool",
    "allowed": [
      "true",
      "false"
    ]
  },
  "server-authoritative-block-breaking": {
    "env": "SERVER_AUTHORITATIVE_BLOCK_BREAKING",
    "type": "bool",
    "allowed": [
      "true",
      "false"
    ]
  },
  "chat-restriction": {
    "env": "CHAT_RESTRICTION",
    "type": "string",
    "allowed": [
      "None",
      "Dropped",
      "Disabled"
    ]
  },
  "disable-player-interaction": {
    "env": "DISABLE_PLAYER_INTERACTION",
    "type": "bool",
    "allowed": [
      "true",
      "false"
    ]
  },
  "client-side-chunk-generation-enabled": {
    "env": "CLIENT_SIDE_CHUNK_GENERATION_ENABLED",
    "type": "bool",
    "allowed": [
      "true",
      "false"
    ]
  },
  "level-type": {
    "env": "LEVEL_TYPE",
    "type": "string",
    "allowed": [
      "DEFAULT",
      "FLAT",
      "LEGACY"
    ],
    "mappings": {
      "flat": "FLAT",
      "legacy": "LEGACY",
      "default": "DEFAULT"
    }
  },
  "white-list": {
    "env": "WHITE_LIST",
    "type": "bool",
    "allowed": [
      "true",
      "false"
    ]
  },
  "emit-server-telemetry": {
    "env": "EMIT_SERVER_TELEMETRY",
    "type": "bool",
    "allowed": [
      "true",
      "false"
    ]
  }
}